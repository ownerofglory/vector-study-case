        "produces": [
          "text/plain"
        ],
        "operationId": "findPairRequest",
        "parameters": [
          {
            "type": "string",
            "example": "1,2,3,4",
            "name": "A",
            "in": "query",
            "required": true
          },
          {
            "type": "string",
            "example": "5,6,7",
            "name": "B",
            "in": "query",
            "required": true
          },
          {
            "type": "integer",
            "format": "int64",
            "name": "Target",
            "in": "query",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": ""
          },
          "400": {
            "description": ""
          }
        }
      }
    }
  }
